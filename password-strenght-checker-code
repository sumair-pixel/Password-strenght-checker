#include<iostream>
using namespace std;
bool isnumerical(char arr[],int check);
bool iscaps(char arr[],int check);
bool issmall(char arr[],int check);
bool isspecial(char arr[],int check);

int main() {
    const char size = 50;
    char arr[size];
    char arrchar[1];
    int check = 0;
    int64_t permutation = 1;
    cout<<"Enter your password  (press / when you have written your password)\n";
    bool flag = true;
    while(flag) {
        for (int i = 0; i < size; i++) {
            cin>>arr[i];
            if (arr[i] == '/') {
                check = i;
                break;
            }
        }
        for (int i = 0; check > i; i++) {
            if ((iscaps(&arr[i], 1) || issmall(&arr[i], 1))) {
                permutation = permutation * 26;
            } else if ((isnumerical(&arr[i], 1))) {
                permutation = permutation * 9;
            } else if ((isspecial(&arr[i], 1))) {
                permutation = permutation * 32;
            }
        }
        if(permutation < 100000000){
            cout<<"your password is weak try again";
        }else if (permutation < 1000000000000){
            cout<<"your password is moderate";
            flag = false;
        }else {
            cout<<"your password is strong";
            flag = false;
        }
    }

    }

bool iscaps(char arr[],int check) {
    for (int i = 0; i < check; i++) {
        if (arr[i] > 64 && arr[i] < 91) {
            return true;
        }
    }
    return false;
}
bool issmall(char arr[],int check) {
    for (int i = 0; i < check; i++) {
        if (arr[i]>96 and arr[i]<123) {
            return true;
        }

    }return false;
}
bool isnumerical(char arr[],int check) {
    for (int i = 0; i < check; i++) {
        if(arr[i] >47 && arr[i] <58) {
            return true;
        }

    }
    return false;
}
bool isspecial(char arr[],int check) {
    int power = 0;
    for (int i = 0; i < check; i++) {
        power = 0;
        if (arr[i] > 64 && arr[i] < 91) {
            power = 1;
        }
        if (arr[i] > 96 && arr[i] < 123) {
            power = 1;
        }
        if (arr[i] > 47 && arr[i] < 58) {
            power = 1;
        }
        if (arr[i] == 32) {
            power = 1;

        }
        if (power == 0) {
            return true;
        }
    }
    return false;
}

